 year = {2016},
 title = {欧美学院教学运行辅助管理系统的设计与实现},
 url = {http://www.wanfangdata.com.cn/details/detail.do?_type=degree&id=D01111379},
 keywords = {高等学校;教学管理;软件开发;数据库;信息管理系统;中外合作办学},
 school = {燕山大学},
 type = {硕士},
 file = {7b72011c-3946-43e9-8892-1c62d8957ef8:C\:\\Users\\25710\\AppData\\Local\\Swiss Academic Software\\Citavi 6\\ProjectCache\\diq77qomutp8sdzssqghijx2sck25xquelhkwn8us7qn08\\Citavi Attachments\\7b72011c-3946-43e9-8892-1c62d8957ef8.pdf:pdf}
}


